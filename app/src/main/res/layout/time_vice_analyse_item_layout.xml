<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="4dp"
    android:layout_marginBottom="4dp"
    android:background="@drawable/input_field_background1"
    android:padding="16dp">

    <TextView
        android:id="@+id/timeViceAnalyseTextViewDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/date_background"
        android:fontFamily="@font/inter_regular"
        android:paddingStart="12dp"
        android:paddingTop="4dp"
        android:paddingEnd="12dp"
        android:paddingBottom="4dp"
        android:text="2025-01-01"
        android:textColor="@color/secondary_color_black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/timeViceAnalyseTextViewRecordTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_regular"
        android:text="Record Time : 12.00 am"
        android:textColor="@color/secondary_color_black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/timeViceAnalyseTextViewDate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/timeViceAnalyseTextViewDate" />

    <LinearLayout
        android:id="@+id/timeViceAnalyseLinearLayout1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/alert_dialog_background"
        android:orientation="vertical"
        android:padding="8dp"
        app:layout_constraintEnd_toStartOf="@+id/timeViceAnalyseLinearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeViceAnalyseTextViewRecordTime">

        <TextView
            android:id="@+id/timeViceAnalyseTextViewSys"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_regular"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="120"
            android:textAlignment="center"
            android:textColor="@color/primary_color_green"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:text="@string/sys"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="@string/mmhg"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="9sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/timeViceAnalyseLinearLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/alert_dialog_background"
        android:orientation="vertical"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/timeViceAnalyseLinearLayout1"
        app:layout_constraintEnd_toStartOf="@+id/timeViceAnalyseLinearLayout3"
        app:layout_constraintStart_toEndOf="@+id/timeViceAnalyseLinearLayout1"
        app:layout_constraintTop_toTopOf="@+id/timeViceAnalyseLinearLayout1">

        <TextView
            android:id="@+id/timeViceAnalyseTextViewPul"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_regular"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="80"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:text="@string/pul"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="@string/min"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="9sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/timeViceAnalyseLinearLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:background="@drawable/alert_dialog_background"
        android:orientation="vertical"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/timeViceAnalyseLinearLayout1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/timeViceAnalyseLinearLayout2"
        app:layout_constraintTop_toTopOf="@+id/timeViceAnalyseLinearLayout1">

        <TextView
            android:id="@+id/timeViceAnalyseTextViewDia"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_regular"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="80"
            android:textAlignment="center"
            android:textColor="@color/primary_color_green"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:text="@string/dia"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/timeViceAnalyseTextView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/inter_light"
            android:paddingStart="12dp"
            android:paddingTop="4dp"
            android:paddingEnd="12dp"
            android:paddingBottom="4dp"
            android:text="@string/mmhg"
            android:textAlignment="center"
            android:textColor="@color/secondary_color_black"
            android:textSize="9sp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>